<div class="row">
    <% events.forEach( function(e) { %>
        <div class="card col-12 col-md-6">
            <h3 class="text-left">
                <a href="/event/<%= e.id %>">
                    <%= e.name %>
                </a>
            </h3>
            <div class="row">
                <div class="col-md-6">
                    <img class="img-fluid img-thumbnail" alt="<%= e.name %>" src="<%= e.url %>">
                </div>
                <div class="col-md-6">
                    <p>
                        <%= e.s_desc %>
                    </p>
                </div>
            </div>
        </div>
        <% }); %>
</div>